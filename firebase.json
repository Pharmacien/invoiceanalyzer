{
  "hosting": {
    "source": ".",
    "rewrites": [
      {
        "source": "**",
        "apphosting": "invoiceanalyzer"
      }
    ]
  },
  "apphosting": [
    {
      "backendId": "invoiceanalyzer",
      "rootDir": "/",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "functions"
      ]
    }
  ]
}