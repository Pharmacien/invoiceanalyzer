{
  "hosting": {
    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "invoiceanalyzer",
          "region": "europe-west4"
        }
      }
    ]
  },
  "apphosting": [
    {
      "backendId": "invoiceanalyzer",
      "rootDir": "/",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "functions",
        ".firebase"
      ]
    }
  ]
}